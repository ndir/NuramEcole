	<queryString language="SQL">
		<![CDATA[Select MONTH(intervention.dateintervention) Mois,nom,prenom,libsecteur,libTypeprestation,libplan,sum(heurefintheorique-heuredebuttheorique) TotalHeure
from intervenant,lstsecteur,intervention,lsttypeprestation,PlanAideBeneficiaire,lstplanaide
where intervenant.idsecteur=lstsecteur.idsecteur
AND intervenant.idintervenant=intervention.idintervenant
AND intervention.idPlanAideBeneficiaire=PlanAideBeneficiaire.idPlanAideBeneficiaire /*Jointure externe a ajouter*/
AND PlanAideBeneficiaire.idplanaide=lstplanaide.idplanaide
AND intervention.idtypeprestation=lsttypeprestation.idTypePrestation
AND MONTH(dateintervention)=MONTH(now())
group by intervenant.idintervenant]]>
	</queryString>
